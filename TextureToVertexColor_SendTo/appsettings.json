{
  // If blank, this app will search for it
  "blender_exe": "",

  // Set to zero if you don't want subdivisions.  More than one makes more triangles (and vertices to paint), but it's
  // gridlike and ugly, so would need to be manually smoothed out
  "subdivide_count": 1,

  // True: each object becomes its own .obj file
  // False: a single .obj is created (shapelab will import that as a single combined object)
  "multi_obj_files": true,

  // If true, this will keep the console window open until a key is pressed (or click the close button)
  // If there was an error, the window will stay open
  "prompt_for_close": true
}